{
  "name": "bosco",
  "version": "0.5.2",
  "description": "Bosco will take care of your microservices, just don't try and use him on a plane.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "f(){ if [ \"$#\" -gt 0 ]; then npm run testonly -- \"$@\"; else npm run check-coverage; fi; };f",
    "testonly": "mocha",
    "coverage": "istanbul cover --include-all-sources true _mocha",
    "check-coverage": "npm run coverage && istanbul check-coverage --statements 16",
    "coveralls": "npm run coverage && npm run coveralls-after-test",
    "coveralls-after-test": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "micro",
    "service",
    "build",
    "automation",
    "minification",
    "s3",
    "project"
  ],
  "bin": {
    "bosco": "./bin/bosco.js"
  },
  "directories": {
    "man": "./man",
    "bin": "./bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tes/bosco.git"
  },
  "author": "clifton.cunningham@tes.co.uk",
  "license": "MIT",
  "dependencies": {
    "appdirectory": "^0.1.0",
    "asciify": "^1.3.5",
    "async": "^1.4.2",
    "clean-css": "^3.1.9",
    "cli-table": "^0.3.0",
    "colors": "^1.0.3",
    "dockerode": "^2.0.3",
    "fs-extra": "^0.26.2",
    "glob": "^6.0.1",
    "handlebars": "^4.0.3",
    "highland": "^2.2.0",
    "inquirer": "^0.10.1",
    "knox": "^0.9.0",
    "lodash": "^3.5.0",
    "mime": "^1.2.11",
    "moment": "^2.8.3",
    "nconf": "^0.8.1",
    "nsp": "^2.0.2",
    "octonode": "^0.7.4",
    "osenv": "^0.1.0",
    "parse-link-header": "^0.4.1",
    "pm2": "^0.15.7",
    "prettyjson": "^1.0.0",
    "progress": "^1.1.8",
    "prompt": "^0.2.13",
    "request": "^2.40.0",
    "semver": "^5.0.3",
    "sf": "^0.1.7",
    "symlink": "^2.0.0",
    "tail": "^0.4.0",
    "tar-fs": "^1.2.0",
    "uglify-js": "^2.6.1",
    "watch": "^0.16.0",
    "yargs": "^3.26.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^0.1.0",
    "expect.js": "^0.3.1",
    "gulp": "^3.8.7",
    "gulp-marked-man": "^0.3.1",
    "istanbul": "^0.3.2",
    "mocha": "^2.2.1",
    "pre-commit": "^1.1.1",
    "sinon": "^1.17.1"
  },
  "config": {
    "blanket": {
      "pattern": "src"
    }
  }
}
